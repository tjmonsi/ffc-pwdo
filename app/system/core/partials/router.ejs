    <%
      const middleware = require('./middleware.ejs')({middleware: config.routes.middleware});
      const routes = require('./routes.ejs')({routes: config.routes.routing});
    %>

    <lazy-loader></lazy-loader>

    <littleq-router default-redirect="<%= config.routes.defaultRedirect %>">
      <%= middleware %>
      <%= routes %>
      <littleq-route page="<%= config.routes.notFound %>"></littleq-route>
    </littleq-router>